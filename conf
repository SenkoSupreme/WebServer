server {
  listen = 8080
  host = 127.0.0.1
  server_name = example.com
  root = /Users/abenouda/Desktop/repo/www
  index = index.html

  location = [
  name = /img
  root = /Users/abenouda/Desktop/repo/img
  method = GET POST DELETE
  ]

  location = [
  name = /basic
  root = /Users/abenouda/Desktop/repo/www/basic-php-website
  location_index = index.php
  method = GET DELETE POST
  autoindex = on
  ]

  location = [
  name = .php
  root = /Volumes/WIFISLAX64-/.brew/opt/php@7.4/bin/php-cgi
  ]

  location = [
  name = .perl
  root = /usr/bin/perl
  ]

  location = [
  name = .py
  root = /usr/bin/python
  ]
}

